{
  "name": "never5/download-monitor",
  "description": "Download Monitor",
  "keywords": [
    "wordpress",
    "download monitor"
  ],
  "homepage": "https://www.download-monitor.com/",
  "license": "GPL-2.0+",
  "author": "Never5",
  "autoload": {
    "psr-4": {
      "Never5\\DownloadMonitor\\": "src/"
    },
    "classmap": [
      "src/",
      "includes/"
    ]
  },
  "require": {
  },
  "require-dev": {
    "pimple/pimple": "*",
    "paypal/paypalhttp": "*",
    "phpunit/phpunit": "^6.4",
    "coenjacobs/mozart": "^0.7.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "mozart": {
      "dep_namespace": "Never5\\DownloadMonitor\\Dependencies\\",
      "dep_directory": "/src/Dependencies/",
      "classmap_directory": "/tmp/mozart",
      "classmap_prefix": "NOTUSED_",
      "packages": [
        "paypal/paypalhttp",
        "pimple/pimple"
      ]
    }
  }
}
