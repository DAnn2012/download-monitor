{
  "name": "download-monitor",
  "description": "Download Monitor",
  "keywords": [
    "wordpress",
    "download monitor"
  ],
  "homepage": "https://www.download-monitor.com/",
  "license": "GPL-2.0+",
  "author": "Never5",
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  },
  "autoload": {
    "classmap": [
      "src/",
      "src/Admin",
      "src/Admin/Settings",
      "src/Admin/Settings/Fields",
      "src/Download",
      "src/Product",
      "src/Widgets",
      "src/Libs/uaparser"
    ]
  },
  "require": {
    "xrstf/composer-php52": "1.*",
    "paypal/rest-api-sdk-php": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "coenjacobs/mozart": "^0.2.2"
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Never5\\DownloadMonitor\\Ecommerce\\Dependencies\\",
      "dep_directory": "/src/Ecommerce/Dependencies/",
      "classmap_directory": "/tmp/mozart",
      "classmap_prefix": "TROEP_",
      "packages": [
        "paypal/rest-api-sdk-php",
        "psr/log"
      ]
    }
  }
}
