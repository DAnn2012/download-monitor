{
  "name": "download-monitor",
  "description": "Download Monitor",
  "keywords": [
    "wordpress",
    "download monitor"
  ],
  "homepage": "https://www.download-monitor.com/",
  "license": "GPL-2.0+",
  "author": "Never5",
  "autoload": {
    "classmap": [
      "src/",
      "src/Admin",
      "src/Admin/Settings",
      "src/Admin/Settings/Fields",
      "src/Download",
      "src/Product",
      "src/Widgets",
      "src/Libs/uaparser"
    ]
  },
  "require": {
    "paypal/rest-api-sdk-php": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "coenjacobs/mozart": "^0.3.0"
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Never5\\DownloadMonitor\\Shop\\Dependencies\\",
      "dep_directory": "/src/Shop/Dependencies/",
      "classmap_directory": "/tmp/mozart",
      "classmap_prefix": "NOTUSED_",
      "packages": [
        "paypal/rest-api-sdk-php"
      ]
    }
  }
}
